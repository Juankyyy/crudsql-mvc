
<table class="table table-dark">
    <tr>
        <td>ID</td>
        <td>Nombres</td>
        <td>Apellidos</td>
        <td>Correo</td>
    </tr>

    @foreach (var user in Model.Users) {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
        </tr>
    }
</table>